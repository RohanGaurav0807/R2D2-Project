In [3]:
import urllib.request
from bs4 import BeautifulSoup
In [8]:
url = urllib.request.urlopen("https://www.imdb.com/search/title/?count=100&groups=top_1000&sort=user_rating")
ht = url.read()
so = BeautifulSoup(ht,"html.parser")
In [9]:
tit=so.title
print(tit)
<title>IMDb "Top 1000"
(Sorted by IMDb Rating Descending) - IMDb</title>
In [10]:
name_of_movie=[]
ratings_of_movie=[]
cast&crew_of_movie=[]
id_of_movie=[]

pages=so.select("div.lister-item-content")
for k in pages:
    name_of_movie.append((k.find("a").text))
    
    ratings_of_movie.append(((k.find('div','inline-block ratings-imdb-rating')).text).split()[0])
    
    id_of_movie.append(((k.find("a"))['href']).split('/')[2])
    
    list = k.findAll('p')
    list = list[2].text
    ind = list.find("Stars:")
    newl = ("".join(list[ind+6:].split('\n'))).strip()
    newl = newl.split(', ')
    cast&crew_of_movie.append(newl)
